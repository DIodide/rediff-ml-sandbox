# Environment Variables Template
# Copy this file to .env and fill in your actual values
# Never commit .env to version control!

# Pinecone Vector Database
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here  # Optional for newer accounts
PINECONE_INDEX_NAME=ml-sandbox-index

# Supabase Configuration
SUPABASE_URL=https://your_project_id.supabase.co
SUPABASE_KEY=your_supabase_anon_or_service_key_here
SUPABASE_DB_URL=postgresql://postgres:[YOUR-PASSWORD]@db.your_project_id.supabase.co:5432/postgres

# Alternative PostgreSQL (if not using Supabase)
DATABASE_URL=postgresql://username:password@localhost:5432/database_name
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# OpenAI (for embeddings, optional)
OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face (for alternative embeddings, optional)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# General Settings
ENVIRONMENT=development  # development, staging, production
LOG_LEVEL=INFO 